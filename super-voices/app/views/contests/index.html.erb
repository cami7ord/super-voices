<p id="notice"><%= notice %></p>

<h1>Contests</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Picture</th>
      <th>Url</th>
      <th>Start</th>
      <th>End</th>
      <th>Payment</th>
      <th>Script</th>
      <th>Guidelines</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contests.each do |contest| %>
      <tr>
        <td><%= contest.name %></td>
        <td><%= contest.picture %></td>
        <td><%= contest.url %></td>
        <td><%= contest.start %></td>
        <td><%= contest.end %></td>
        <td><%= contest.payment %></td>
        <td><%= contest.script %></td>
        <td><%= contest.guidelines %></td>
        <td><%= link_to 'Show', contest %></td>
        <td><%= link_to 'Edit', edit_contest_path(contest) %></td>
        <td><%= link_to 'Destroy', contest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contest', new_contest_path %>
